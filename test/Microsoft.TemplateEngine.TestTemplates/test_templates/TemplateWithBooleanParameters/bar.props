<!-- MSBuild processing -->
<!-- Values -->
<ValueA>VAL_A</ValueA>
<ValueB>VAL_B</ValueB>
<ValueC>VAL_C</ValueC>
<ValueD>VAL_D</ValueD>

<!--'$(A)' in condition -->
<TargetFramework Condition="'$(A)'">A-true</TargetFramework>
<TargetFramework Condition="'$(B)'">B-true</TargetFramework>
<TargetFramework Condition="'$(C)'">C-true</TargetFramework>
<TargetFramework Condition="'$(D)'">D-true</TargetFramework>
<!--'$(A)' == true in condition -->
<TargetFramework Condition="'$(A)' == true">A-true</TargetFramework>
<TargetFramework Condition="'$(B)' == true">B-true</TargetFramework>
<TargetFramework Condition="'$(C)' == true">C-true</TargetFramework>
<TargetFramework Condition="'$(D)' == true">D-true</TargetFramework>
<!--'$(A)' == 'true' in condition -->
<TargetFramework Condition="'$(A)' == 'true'">A-true</TargetFramework>
<TargetFramework Condition="'$(B)' == 'true'">B-true</TargetFramework>
<TargetFramework Condition="'$(C)' == 'true'">C-true</TargetFramework>
<TargetFramework Condition="'$(D)' == 'true'">D-true</TargetFramework>

