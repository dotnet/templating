    "Framework": {
      "type": "parameter",
      "datatype": "choice",
      "choices": [
        {
          "choice": "netcoreapp1.0",
          "description": "Target netcoreapp1.0"
        },
        {
          "choice": "netcoreapp1.1",
          "description": "Target netcoreapp1.1"
        },
        {
          "choice": "netstandard2.0",
          "description": "Target netstandard2.0"
        }
      ],
      "replaces": "netcoreapp1.0",
      "defaultValue": "netcoreapp1.0"
    },
    "IsNetStandard2": {
      "type": "computed",
      "value": "(Framework == 'netcoreapp2.0' || Framework == 'netstandard2.0')"
    }
  }
