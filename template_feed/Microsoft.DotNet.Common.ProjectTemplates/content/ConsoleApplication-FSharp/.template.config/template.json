{
  "author": "Microsoft",
  "classifications": ["Common", "Console"],
  "name": "Console Application",
  "groupIdentity": "Microsoft.Common.Console",
  "identity": "Microsoft.Common.Console.FSharp",
  "shortName": "console",
  "tags": {
    "language": "F#",
    "type": "project"
  },
  "sources": [
    {
      "modifiers": [
        {
