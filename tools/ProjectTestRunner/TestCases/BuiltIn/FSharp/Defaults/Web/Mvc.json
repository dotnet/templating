{
  "skip": true,
  "create": "mvc -f netcoreapp2.0 --language F# --no-restore",
  "name": "MVC (F#, 2.0 framework, default auth)",
  "tasks": [
    {
      "handler": "execute",
      "command": "dotnet",
      "args": "restore -s https://pkgs.dev.azure.com/dnceng/public/_packaging/myget-legacy/nuget/v3/index.json -s https://dotnetmygetlegacy.blob.core.windows.net/dotnet-core/index.json -s https://api.nuget.org/v3/index.json",
      "noExit": false,
      "exitCode": 0
    },
    {
      "handler": "execute",
      "command": "dotnet",
      "args": "build",
      "noExit": false,
      "exitCode": 0
    },
    {
      "name": "RunApp",
      "handler": "execute",
      "command": "dotnet",
      "args": "exec bin/Debug/netcoreapp2.0/%targetPathName%.dll",
      "noExit": true,
      "exitTimeout": 5000
    },
    {
      "handler": "httpRequest",
      "url": "http://localhost:5000",
      "statusCode": 200,
      "verb": "GET"
    },
    {
      "handler": "httpRequest",
      "url": "http://localhost:5000/Home/About",
      "statusCode": 200,
      "verb": "GET"
    },
    {
      "handler": "httpRequest",
      "url": "http://localhost:5000/Home/Contact",
      "statusCode": 200,
      "verb": "GET"
    },
    {
      "handler": "taskkill",
      "name": "RunApp"
    }
  ]
}
