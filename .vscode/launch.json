  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run template scanner",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}\\artifacts\\bin\\Microsoft.TemplateSearch.TemplateDiscovery\\Debug\\net9.0\\Microsoft.TemplateSearch.TemplateDiscovery.dll",
      "args": [
        "--basePath",
        "./templates",
        "--verbose",
        "--savePacks",
        "--diff",
        "true",
        "--test",
        "--allowPreviewPacks"
      ]
    }
  ]
}